{
  "name": "@mini-markdown/material",
  "version": "1.0.0",
  "private": true,
  "description": "mini-markdown-editor的物料组件库",
  "keywords": [
    "物料",
    "组件"
  ],
  "type": "module",
  "main": "./dist/mini-markdown-material.umd.cjs",
